{
  "install": {
    "include": [
      "^package\\.json$"
    ],
    "exclude": [
      "^\\.md$"
    ]
  },
  "restart": {
    "include": [
      "^\\.env$",
      "^backend/"
    ],
    "exclude": [
      "public/",
      "vite.config.js",
      "settings.json",
      "^\\.md$"
    ]
  },
  "noSavedEvents": true
}
